{
  "name" : "mt-olympus/demeter",
  "require" : {
    "zendframework/zend-expressive" : "1.0.0rc2",
    "zendframework/zend-stdlib" : "~2.7",
    "zendframework/zend-expressive-fastroute" : "^0.2",
    "zendframework/zend-servicemanager" : "^2.5",
    "los/los-rate-limit": "^0.9.1"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^4.8",
    "squizlabs/php_codesniffer" : "^2.3"
  },
  "license" : "MIT",
  "autoload-dev" : {
    "psr-4" : {
      "DemeterTest\\" : "test/"
    }
  },
  "autoload" : {
    "psr-4" : {
      "Demeter\\" : "src/"
    }
  },
  "type" : "application",
  "scripts" : {
    "test" : [ "php vendor/squizlabs/php_codesniffer/scripts/phpcs", "php vendor/phpunit/phpunit/phpunit" ]
  },
  "homepage" : "https://github.com/mt-olympus/demeter",
  "authors" : [ {
    "name" : "Leandro Silva",
    "homepage" : "http://leandrosilva.info/"
  } ]
}
